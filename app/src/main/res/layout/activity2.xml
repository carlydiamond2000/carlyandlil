<?xml version="1.0" encoding="utf-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <TextView
        android:id="@+id/homepage"
        android:layout_width="119dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:text="My Closet"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/homepage2"
        android:layout_width="105dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="84dp"
        android:text="School Outfits"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/homepage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/homepage3"
        android:layout_width="105dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="128dp"
        android:text="Beach Outfits"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homepage4"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/homepage4"
        android:layout_width="105dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="120dp"
        android:text="Work Outfits"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homepage2"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/homepage5"
        android:layout_width="105dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="104dp"
        android:text="Evening Outfits"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homepage3"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/myImageView14"
        android:layout_width="61dp"
        android:layout_height="68dp"
        android:layout_marginStart="250dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/beachfit4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/myImageView16"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/myImageView16"
        android:layout_width="64dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/workfit6"
        app:layout_constraintStart_toEndOf="@+id/myImageView9"
        app:layout_constraintTop_toTopOf="@+id/myImageView9" />

    <ImageView
        android:id="@+id/myImageView8"
        android:layout_width="64dp"
        android:layout_height="71dp"
        android:layout_marginStart="24dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/workfit2"
        app:layout_constraintEnd_toStartOf="@+id/myImageView9"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homepage4" />

    <ImageView
        android:id="@+id/myImageView9"
        android:layout_width="56dp"
        android:layout_height="67dp"
        android:layout_marginStart="16dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/workfit3"
        app:layout_constraintStart_toEndOf="@+id/myImageView8"
        app:layout_constraintTop_toTopOf="@+id/myImageView8" />


    <ImageView
        android:id="@+id/navbarbackground"
        android:layout_width="426dp"
        android:layout_height="101dp"
        android:layout_marginBottom="4dp"
        android:adjustViewBounds="true"
        android:background="#E7F9EF"
        android:contentDescription="@string/todo1"
        android:contextClickable="false"
        android:cropToPadding="false"
        android:src="@drawable/navbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/camerabutton"
        android:layout_width="121dp"
        android:layout_height="80dp"
        android:layout_marginStart="140dp"
        android:layout_marginBottom="16dp"
        android:background="#E7F9EF"
        android:contentDescription="@string/a_tiny_android_icon4"
        android:src="@drawable/navigation_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/navbarbackground"
        app:layout_constraintTop_toTopOf="@+id/closetbtn"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/profileb"
        android:layout_width="121dp"
        android:layout_height="80dp"
        android:layout_marginStart="292dp"
        android:background="#E7F9EF"
        android:contentDescription="@string/a_tiny_android_icon3"
        android:src="@drawable/profile_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/camerabutton"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/closetbtn"
        android:layout_width="121dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:background="#E7F9EF"
        android:contentDescription="@string/a_tiny_android_icon2"
        android:src="@drawable/closet_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview"
        android:layout_width="379dp"
        android:layout_height="93dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/homepage4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homepage2"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>